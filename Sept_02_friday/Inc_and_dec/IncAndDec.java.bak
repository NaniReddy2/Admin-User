class IncAndDec 
{
	public static void main(String[] args) 
	{
		double a=2.5;
		int b=31;
		char c='B';
		System.out.println(a++ - b-- + c++ + --a);
		System.out.println(b-- + --a - --c + a++);
		System.out.println(c++ - a-- + b-- + --b);
		System.out.println(++c + ++c + --a + --b);
		System.out.println(++c - a-- + b++ + a*2);
		System.out.println(a/b + --a + b*2+1 + ++a);
	}
}
