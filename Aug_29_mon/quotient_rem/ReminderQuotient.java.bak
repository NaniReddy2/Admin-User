class ReminderQuotient 
{
	public static void main(String[] args) 
	{ 
		int a=10;
		int b=20;
		int c=a+b;
		int d=a-b;
		int e=a*b*c;
		int f=b/a;
		int g=a%b;


		System.out.println("add"+a+"the"+b+"two no."+c);
		System.out.println("sub"+a+"the"+b+"two no."+d);
		System.out.println("mul"+a+"the"+b+"three no."+e);
		System.out.println("quotient"+a+"the"+b+"two no."+f);
		System.out.println("rem"+a+"the"+b+"two no."+g);
	}
}
